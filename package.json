{
  "name": "generative-sullivan-ornament",
  "version": "0.1.0",
  "description": "Plate-inspired generative ornament prototype",
  "main": "dist/electron/main.js",
  "scripts": {
    "build": "tsc && npm run build:renderer",
    "build:renderer": "esbuild src/renderer/controllers.tsx src/renderer/preview.ts --bundle --outdir=public --format=iife --platform=browser --sourcemap --jsx=automatic",
    "start": "npm run build && electron .",
    "start:cli": "ts-node src/index.ts",
    "demo:plate2:leaf": "ts-node src/plate2_leaf_demo.ts",
    "dev:renderer": "esbuild src/renderer/controllers.tsx src/renderer/preview.ts --bundle --outdir=public --format=iife --platform=browser --sourcemap --jsx=automatic --watch"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reactflow": "^11.10.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.0",
    "electron": "^39.2.4",
    "esbuild": "^0.27.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
